    "@radix-ui/react-slot": "^1.1.1",
    "@tanstack/react-table": "^8.20.6",
    "better-auth": "^1.1.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "lucide-react": "^0.474.0",
    "minio": "^8.0.4",
    "next": "14.2.16",
    "next-themes": "^0.4.4",
    "nuqs": "^2.3.1",
